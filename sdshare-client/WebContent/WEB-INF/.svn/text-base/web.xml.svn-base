<?xml version="1.0" encoding="ISO-8859-1" ?>

<web-app xmlns="http://java.sun.com/xml/ns/j2ee" version="2.4">

  <display-name>SDshare client</display-name>
  <description>
    SDshare client for receiving changes from other SDshare servers.
  </description>

  <servlet>
    <servlet-name>Startup</servlet-name>
    <servlet-class>
      net.ontopia.topicmaps.utils.sdshare.client.StartupServlet
    </servlet-class>
    <load-on-startup>-1</load-on-startup> <!-- 1 == on, but also 0 == on -->
    <init-param>
      <param-name>app-name</param-name>
      <param-value>sdshare-client</param-value>
    </init-param>
  </servlet>
  
</web-app>
